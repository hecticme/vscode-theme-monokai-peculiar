{
  "publisher": "hecticme",
  "name": "monokai-peculiar",
  "displayName": "Monokai Peculiar",
  "description": "VSCode theme with Monokai as the base",
  "icon": "icon.png",
  "version": "0.2.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hecticme/vscode-theme-monokai-peculiar"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "monokai",
    "dark"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Monokai Peculiar",
        "uiTheme": "vs-dark",
        "path": "./themes/default.json"
      }
    ]
  },
  "type": "module",
  "scripts": {
    "build": "node src/index.js",
    "lint": "eslint .",
    "publish": "npm run build && vsce publish"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@vscode/vsce": "^2.26.1",
    "eslint": "^9.3.0"
  }
}
